<UserControl x:Class="CSWPF.MVVM.View.HomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:CSWPF.MVVM.View"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        
        <TextBlock Grid.Row="0" Text="Home" Foreground="White" FontSize="28" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,0,0,20"/>
        
        <StackPanel Grid.Row="1" Name="PanelForStart" Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="5"/>
        
    </Grid>
</UserControl>
